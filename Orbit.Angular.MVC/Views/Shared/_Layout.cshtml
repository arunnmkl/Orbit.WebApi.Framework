<!DOCTYPE html>
<html data-ng-app="AngularMVCApp">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="data:image/png;base64," rel="icon" type="image/x-icon" />
    <title>Orbit Angular MVC Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation" data-ng-controller="indexController">
        <div class="container">
            <div class="navbar-header">
                <button class="btn btn-success navbar-toggle" data-ng-click="navbarExpanded = !navbarExpanded">
                    <span class="glyphicon glyphicon-chevron-down"></span>
                </button>
                <a class="navbar-brand" href="#/">Home</a>
            </div>
            <div class="collapse navbar-collapse" data-collapse="!navbarExpanded">
                <ul class="nav navbar-nav navbar-right">
                    <li data-ng-hide="!authentication.isAuth"><a href="#/welcome">Welcome {{authentication.userName}}</a></li>
                    <li data-ng-hide="!authentication.isAuth"><a href="#/orders">My Orders</a></li>
                    <li data-ng-hide="!authentication.isAuth"><a href="#/chat">Chat</a></li>
                    <li data-ng-show="canToken('Read_RefreshToken') && authentication.isAuth"><a href="#/refresh">Refresh Token</a></li>
                    <li data-ng-hide="!authentication.isAuth"><a href="" data-ng-click="logOut()">Logout</a></li>
                    <li data-ng-hide="authentication.isAuth"> <a href="#/login">Login</a></li>
                    <li data-ng-hide="authentication.isAuth"> <a href="#/signup">Sign Up</a></li>
                </ul>
            </div>
        </div>
    </div>
    @RenderBody()
    <hr />
    <div id="footer">
        <div class="container">
            <div class="row">
                <div class="col-md-9">
                    <p class="text-muted"></p>
                </div>
                <div class="col-md-3">
                    <p class="text-muted">Created by <a href="mailto:arun.kumar@dreamorbit.com?Subject=Orbit Framework">Arunkumar Karunakaran</a></p>
                </div>
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @{
        var baseUri = System.Configuration.ConfigurationManager.AppSettings["ChatBaseUri"] ?? "http://localhost:50534";
        var chatScriptFormat = "<script src=\"" + baseUri + "{0}\" type=\"text/javascript\"></script>";
    }
    @Scripts.RenderFormat(chatScriptFormat, "/Scripts/jquery.signalR-2.2.1.js", string.Concat(System.Configuration.ConfigurationManager.AppSettings["ChatBasePath"] ?? "/signalR", "/hubs"))
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/App")
    @Scripts.Render("~/bundles/Modules")
    @Scripts.Render("~/bundles/Directives")
    @Scripts.Render("~/bundles/Services")
    @Scripts.Render("~/bundles/Controllers")
    @RenderSection("scripts", required: false)
</body>
</html>
